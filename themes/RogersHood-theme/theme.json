	"styles": {
		"color": {},
		"elements": {
			"h1": {
				"typography": {
					"fontSize": "clamp(1.625rem, 2vw + 1rem, 3.25rem)",
					"fontFamily": "Scilla",
					"fontWeight": "500"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "clamp(1.625rem, 2vw + 1rem, 2.25rem)",
					"fontFamily": "Scilla",
					"fontWeight": "500"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "clamp(1.125rem, 2vw + 1rem, 1.375rem)",
